// Enum for defining the status of a page
enum PageStatus {
    PUBLISHED // ACTIVE
    ARCHIVED // INACTIVE
    TRASHED // REMOVED
}

// Enum for defining the general status of an entity
enum Status {
    ACTIVE
    INACTIVE
    TRASHED
}

// Enum for defining the origin of OTP
enum otpOrigin {
    MFA_Login
    forgot_Pass
    NULL
}

// Enum for defining the log outcome
enum logOutcome {
    Success
    Failure
    Unknown
}

// Enum for defining the action performed by user
enum logAction {
    CREATE
    UPDATE
    DELETE
    LOGIN
    LOGOUT
    NULL
}
